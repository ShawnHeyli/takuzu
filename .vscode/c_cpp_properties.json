{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "defines": [],
            "compilerPath": "/usr/bin/gcc",
            "intelliSenseMode": "linux-gcc-x64",
            "cStandard": "c99",
            "compilerArgs": [
                "-Wall",
                "-Wextra",
                "-Wshadow",
                "-Wstrict-prototypes",
                "-Wswitch-default",
                "-Wconversion",
                "-Wunreachable-code",
                "-pedantic",
                "-lm",
                "-lrt",
                "-Iinclude"
            ]
        }
    ],
    "version": 4
}